{
  "schemaVersion": 1,
  "id": "troll_commands",
  "version": "${version}",
  "name": "Troll Commands",
  "description": "This mod adds a bunch of commands you can use to troll your friends!",
  "authors": [
    "SmushyTaco"
  ],
  "contact": {
    "homepage": "https://www.smushytaco.com/",
    "issues": "https://www.curseforge.com/minecraft/mc-mods/troll_commands/issues/",
    "sources": "https://github.com/realSmushyTaco/Troll-Commands/"
  },
  "license": "GNU Lesser General Public License version 3 (LGPLv3)",
  "icon": "assets/troll_commands/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.troll_commands.TrollCommands"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.troll_commands.TrollCommandsClient"
      }
    ],
    "modmenu": [
      "com.smushytaco.troll_commands.configuration_support.ModMenuIntegration"
    ]
  },
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.6.0+kotlin.1.5.0",
    "minecraft": "1.16.x",
    "cloth-config2": ">=4.11.24"
  },
  "suggests": {
    "modmenu": ">=1.14.13+build.22"
  }
}